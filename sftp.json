[
  {
    "name": "A",
    "protocol": "sftp",
    "host": "__host__",
    "username": "__username__",
    "remotePath": "/home/__username__/workspace/__dir__",
    "privateKeyPath": "~/.ssh/id_rsa",
    "uploadOnSave": true,
    "profiles": {
      "upload": {
        "ignore": [
          ".vscode",
          ".git",
          ".gitignore",
          "package-lock.json",
          ".DS_Store",
          "node_modules",
          "obj",
          "sftp.json"
        ]
      }
    },
    "defaultProfile": "upload"
  }
]
